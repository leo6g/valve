	<if test="station !=''"> station, </if>
	<if test="result !=''"> result, </if>
	<if test="barcode !=''"> barcode, </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      #{id},
     <if test="testtimes !=''"> #{testtimes}, </if>
	<if test="endtime !=''"> #{endtime}, </if>
	<if test="enda !=''"> #{enda}, </if>
	<if test="starttime !=''"> #{starttime}, </if>
	<if test="starta !=''"> #{starta}, </if>

	<if test="testa !=''"> #{testa}, </if>
	<if test="factorynumber !=''"> #{factorynumber}, </if>
	<if test="machinetype !=''"> #{machinetype}, </if>
	<if test="linenum !=''"> #{linenum}, </if>
	<if test="wpnum !=''"> #{wpnum}, </if>
	<if test="creattime !=''"> #{creattime}, </if>
	<if test="batchnum !=''"> #{batchnum}, </if>
	<if test="stationnum !=''"> #{stationnum}, </if>
	<if test="port !=''"> #{port}, </if>
	<if test="station !=''"> #{station}, </if>
	<if test="result !=''"> #{result}, </if>
	<if test="barcode !=''"> #{barcode}, </if>
    </trim>
  </insert>
  

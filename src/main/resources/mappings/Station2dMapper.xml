	 </if>
	<if test="creattime !=''" >
		 AND creattime = #{creattime}
	 </if>
	<if test="batchnum !=''" >
		 AND batchnum = #{batchnum}
	 </if>
	<if test="stationnum !=''" >
		 AND stationnum = #{stationnum}
	 </if>
	<if test="port !=''" >
		 AND port = #{port}
	 </if>
	<if test="station !=''" >
		 AND station = #{station}
	 </if>
	<if test="result !=''" >
		 AND result = #{result}
	 </if>
	<if test="barcode !=''" >
		 AND barcode = #{barcode}
	 </if>
	     
    </trim>
  </sql>
  
  <sql id="Base_Column_List" >
  	TESTTIMEs,ENDTIME,ENDA,STARTTIME,STARTA,MACHINENUM,testa,factorynumber,MACHINEtype,linenum,wpnum,id,creattime,batchnum,stationnum,port,station,result,barcode
  </sql>
  
  <!-- 分页查询电性能检测工位 -->
  
  <select id="getList" resultMap="BaseResultMap" >
  	
	    select 
	    <include refid="Base_Column_List" />
	    from STATION2D
	<if test="_parameter !=''" >
     	<include refid="Example_Where_Clause" />
   	</if>
    
  </select>
  
  <!-- 根据Id查询电性能检测工位 -->
  
  <select id="getById" resultMap="BaseResultMap" parameterType="java.lang.String" >
    select 
    <include refid="Base_Column_List" />
    from STATION2D
    where id = #{id,jdbcType=VARCHAR}
  </select>
  
  <!-- 查询电性能检测工位总数量 -->
  
  <select id="countAll" resultType="java.lang.Integer" >
	    select count(1)
	    from STATION2D
	    <!-- 有DELETE_FLAG放开-->
	   <!-- where DELETE_FLAG = 0 -->
  </select>
  
  <!-- 查询所有电性能检测工位 -->
  
  <select id="getAll" resultMap="BaseResultMap">
    select 
     <if test="distinct" >
      distinct
    </if>
    <include refid="Base_Column_List" />
    from STATION2D 
   <if test="_parameter !=''" >
      <include refid="Example_Where_Clause" />
    </if>
    <if test="orderByClause !=''" >
      order by ${orderByClause}
    </if>
  </select>

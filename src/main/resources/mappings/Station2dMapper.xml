    <trim prefix="where" prefixOverrides="and|or" >
      <if test="testtimes !=''" >
		 AND TESTTIMEs = #{testtimes}
	 </if>
	<if test="endtime !=''" >
		 AND ENDTIME = #{endtime}
	 </if>
	<if test="enda !=''" >
		 AND ENDA = #{enda}
	 </if>
	<if test="starttime !=''" >
		 AND STARTTIME = #{starttime}
	 </if>
	<if test="starta !=''" >
		 AND STARTA = #{starta}
	 </if>
	<if test="machinenum !=''" >
		 AND MACHINENUM = #{machinenum}
	 </if>
	<if test="testa !=''" >
